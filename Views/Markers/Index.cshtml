                    <td>
                        @Html.DisplayFor(modelItem => item.Gender)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Title)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Persal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MaidenName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nationality)
                    </td>
                  
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.IdentityNo">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.IdentityNo">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.IdentityNo">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
    <script>function scrollWin() {
        window.scrollTo(500, 0);
    }</script>