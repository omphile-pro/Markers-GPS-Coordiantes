@model IEnumerable<Markers_GPS_Coordiantes.Data.Marker>

@{
    ViewData["Title"] = "Index";
}

    <h1>Index</h1>

    <p>
        <a method="get"asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.MarkerToken)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Surname)
