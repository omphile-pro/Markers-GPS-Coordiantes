            </div>
            <div class="form-group">
                <label asp-for="PhysicalAddress" class="control-label"></label>
                <input asp-for="PhysicalAddress" class="form-control" />
                <span asp-validation-for="PhysicalAddress" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LastModifiedByUsersId" class="control-label"></label>
                <input asp-for="LastModifiedByUsersId" class="form-control" />
                <span asp-validation-for="LastModifiedByUsersId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CreatedByUsersId" class="control-label"></label>
                <input asp-for="CreatedByUsersId" class="form-control" />
                <span asp-validation-for="CreatedByUsersId" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsDeleted" /> @Html.DisplayNameFor(model => model.IsDeleted)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                    <a asp-action="Index">Back to List</a>
            </div>
        </form>
    </div>
</div>


