                                            <label for="fax" class="col-sm-3 col-form-label">Fax</label>
                                            <div class="col-sm-9">
                                                <input type="number" class="form-control" id="fax">
                                                <input asp-for="FaxNo" type="number" class="form-control" id="cell">
                                            </div>
                                        </div>
                                        <div class="row row line-head ">

                                        </div>
                                    </div>

                                </div>

                            </div>


                            <i class="fas fa-chevron-left light-blue previous"></i>
